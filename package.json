    "@woocommerce/dependency-extraction-webpack-plugin": "2.2.0",
    "@woocommerce/e2e-environment": "0.2.3",
    "@woocommerce/e2e-utils": "0.2.0",
    "@woocommerce/eslint-plugin": "1.1.0",
    "@woocommerce/experimental": "3.2.0",
